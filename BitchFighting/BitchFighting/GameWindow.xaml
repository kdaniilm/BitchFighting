<Window x:Class="BitchFighting.GameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BitchFighting"
        mc:Ignorable="d"
        Title="GameWindow" Height="450" Width="462.5">
    <Grid>
        <Image Name="LeftHero" HorizontalAlignment="Left" Height="150" Margin="10,10,0,0" VerticalAlignment="Top" Width="200"/>
        <Image Name="RightHero" HorizontalAlignment="Left" Height="150" Margin="242,10,0,0" VerticalAlignment="Top" Width="200"/>
        <Button Name="LeftAttack" Content="Атака 1" Click="LeftAttack_Click" HorizontalAlignment="Left" Margin="10,165,0,0" VerticalAlignment="Top" Width="200" Height="35"/>
        <Button Name="RightAttack" Content="Атака 2" Click="RightAttack_Click" IsEnabled="False" HorizontalAlignment="Left" Margin="242,165,0,0" VerticalAlignment="Top" Width="200" Height="35"/>
        <ListBox Name="LogBox" HorizontalAlignment="Left" Height="191" Margin="10,218,0,0" VerticalAlignment="Top" Width="432"/>
    </Grid>
</Window>
